{
  "name": "baum/baum",
  "type": "library",
  "description": "Baum is an implementation of the Nested Set pattern for Eloquent models.",
  "keywords": ["nested set", "laravel", "laravel 5", "eloquent", "database"],
  "license": "MIT",
  "authors": [
    {
      "name": "Estanislau Trepat",
      "email": "estanis@etrepat.com",
      "homepage": "https://etrepat.com"
    }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0",
    "illuminate/contracts": "~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0",
    "illuminate/database": "~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0",
    "illuminate/events": "~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0",
    "illuminate/support": "~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^7.5|^8.0|^9.0",
    "psy/psysh": "@stable",
    "squizlabs/php_codesniffer": "^3.4",
    "illuminate/console": "~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0",
    "illuminate/filesystem": "~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Baum\\": "src/"
    }
  },
  "autoload-dev": {
      "files": [
        "tests/suite/Support/helpers.php"
      ],
      "psr-4": {
          "Baum\\Tests\\": "tests/suite/"
      }
  },
  "extra": {
    "laravel": {
        "providers": [
          "Baum\\BaumServiceProvider"
        ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
